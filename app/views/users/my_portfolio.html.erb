<h1>My Portfolio</h1>
<div class="mt-5">
    <h2 class="text-center">Search Stocks</h2>
    <%= form_with url:"search_stock", local: true,  method: :get, id: "searchStock", class:"row d-flex justify-content-center" do |f| %>
    <div class="col-md-6">
        <div class="input-group">
            <%= f.text_field :ticker, class: "form-control form-control-lg" %>
            <%= image_submit_tag "search.svg", class: "btn btn-primary rounded-end"%>
        </div>
    </div>
    <% end %>
</div>
<div class="container d-flex justify-content-center">
    <ul class = "list-group" id= "portolfioStocks">
    </ul>
</div>

